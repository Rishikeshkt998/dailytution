<%-include('admin-header')-%>
<div class="container w-25 border py-5 mt-3"style="background-color:#fff">

    <h2><span style="color: rgb(219, 219, 68);">Add</span> <span style="color: rgb(26, 26, 140);">Users</span></h2>
    <form action="/route/add" method="post"class="pt-5">
        <div class="form-group">
            <input type="name" class="form-control" placeholder="username" name="name" required>
        
        </div><br>
        <div class="form-group">
            <input type="text" class="form-control" placeholder="email" name="email" required>
        </div><br>
        <div class="form-group">
            <input type="text" class="form-control" placeholder="Phone number" name="phone" required>
        </div><br>
        <div class="form-group">
            <input type="password" class="form-control" placeholder="password" name="password" required>
        </div><br>
        <button type="submit" class="btn btn-success rounded-pill" style="background-color: rgb(45, 107, 241);border-color:rgb(45, 107, 241) ;">Submit</button>
        
       
    </form>
 
    
    
        

    
    
</div>
<%-include('admin-footer')-%>